    script(src="https://code.jquery.com/jquery-3.3.1.slim.min.js",integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo", crossorigin="anonymous")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js", integrity="sha384-cs/chFZiN24E4KMATLdqdvsezGxaGsi4hLGOzlXwp5UZB1LY//20VyM2taTB4QvJ", crossorigin="anonymous")
    script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js", integrity="sha384-uefMccjFJAIv6A+rW+L4AHf99KvxDjWSu1z9VI8SKNVmz4sk7buKt/6v9KI65qnm", crossorigin="anonymous")
    link(rel="stylesheet", href="https://fonts.googleapis.com/icon?family=Material+Icons")
    link(rel="stylesheet", href="https://code.getmdl.io/1.3.0/material.blue_grey-light_blue.min.css")
    script(defer, src="https://code.getmdl.io/1.3.0/material.min.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/particlesjs/2.2.2/particles.min.js")
  body
    canvas.background-particle
    script(src="/javascripts/particle.js")
    nav.navbar.theme-dark
      a.navbar-brand(href="/").blog-title Potato Coder
    .hero-banner.w-100
      .row.quotes
        .col-md-4.col-sm-12.quote-container
          .quote
            h3.text-light.text-center " I keep on making what I can't do yet in order to learn to be able to do it. "
            p.text-light.text-right - Vincent van Gogh
        .col-md-4.col-sm-12.quote-container
          .quote
            h3.text-light.text-center " Do not let what you cannot do interfere with what you can do. "
            p.text-light.text-right - John Wooden
        .col-md-4.col-sm-12.quote-container
          .quote
            h3.text-light.text-center " As long as you can start, you are all right. The juice will come. "
            p.text-light.text-right - Ernest Hemingway
      block content
      footer.mdl-mini-footer.theme-dark
        .mdl-mini-footer__left-section
          .mdl-logo &copy; Greg Funtera 2018
    
    
    
